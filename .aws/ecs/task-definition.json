{
  "family": "ai-habit-track-task-family",
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::352324842757:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::352324842757:role/my-app-task-role",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "ai-track-app-main-service-container-1",
      "image": "REPLACED_BY_GITHUB_ACTIONS",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ai-habit-track",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:8080/api/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}